{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-alert": 0,
    "consistent-return": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/self-closing-comp": 0,
    "react/jsx-wrap-multilines": 0,
    "react/no-string-refs": 0,
    "react/no-array-index-key": 0
  }
}
